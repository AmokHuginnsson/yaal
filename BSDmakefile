# This is wrapper around GNU make.

all: banner
	@gmake all

nodebug: banner
	@gmake nodebug

dep: banner
	@gmake dep

tags: banner
	@gmake tags

install:
	@gmake install

clean: banner
	@gmake clean

mrproper: banner
	@gmake mrproper

purge: banner
	@gmake purge

banner:
	@echo "--- It is only a wrapper, use GNU make (gmake). ---"

